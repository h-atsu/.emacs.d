#include <algorithm>
#include <bitset>
#include <cmath>
#include <ctime>
#include <iostream>
#include <list>
#include <map>
#include <math.h>
#include <numeric>
#include <queue>
#include <set>
#include <stack>
#include <stdio.h>
#include <string>
#include <vector>
#define rep(i,n) for(int i = 0; i < (n); i++)
#define all(a) a.begin(),a.end()
using namespace std;
using ll = long long;
using P = pair<int,int>;
using vi = vector<int>;
using vvi = vector<vi>;






int T;
ll N,A,B,C,D,ans;



// val 今の数のあたい，tmp　暫定コイン消費量
void solve(ll val, ll tmp, int see) {
  cout << val << " " <<  tmp   << endl; 
  ans = min(ans,abs(val-N)*D + tmp);
  if(val >=N){
    return;
  }
  
  if(see == 2) {
    solve(val*2, min(tmp+A,D*val*2), 2);
    solve(val*3, min(tmp+B,D*val*3), 3);
    solve(val*5, min(tmp+C,D*val*5), 5);
  }
  if(see == 3) {
    solve(val*3, min(tmp+B,D*val*3), 3);
    solve(val*5, min(tmp+C,D*val*5), 5);
  }
  if(see == 5) {
    solve(val*5, min(tmp+C,D*val*5), 5);
  }
  
}



int main() {









 
  return 0;
}








