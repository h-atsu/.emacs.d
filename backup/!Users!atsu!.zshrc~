
# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/Users/atsu/opt/anaconda3/bin/conda' 'shell.zsh' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/Users/atsu/opt/anaconda3/etc/profile.d/conda.sh" ]; then
        . "/Users/atsu/opt/anaconda3/etc/profile.d/conda.sh"
    else
        export PATH="/Users/atsu/opt/anaconda3/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<

#Prompt setting
PS1="%1~ $ "
PATH=$PATH:/Applications/CPLEX_Studio129/cplex/bin/x86-64_osx/